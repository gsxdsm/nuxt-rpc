{
  "name": "nuxt-rpc",
  "version": "0.3.4",
  "packageManager": "pnpm@8.15.9",
  "description": "Remote Functions. Instead of Event Handlers.",
  "keywords": [
    "nuxt",
    "remote",
    "rpc",
    "http",
    "h3",
    "vue"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./server": {
      "import": "./dist/runtime/server.js",
      "types": "./dist/runtime/server.d.ts"
    },
    "./client": {
      "import": "./dist/runtime/client.js",
      "types": "./dist/runtime/client.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "prepack": "pnpm dev:prepare && nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm prepack",
    "release": "changelogen --release && npm publish && git push --follow-tags",
    "update-deps": "taze -w && pnpm i",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepare": "cd playground && npx prisma migrate dev --name init"
  },
  "dependencies": {
    "@nuxt/kit": "^3.14.1592",
    "@rollup/pluginutils": "^5.1.3",
    "dedent": "^1.5.3",
    "defu": "^6.1.4",
    "es-module-lexer": "^1.5.4",
    "fast-glob": "^3.3.2",
    "formdata-node": "^6.0.3",
    "h3": "^1.13.0",
    "nuxt-rpc": "workspace:*",
    "pathe": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.7.2",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.14.1592",
    "@nuxt/test-utils": "^3.15.1",
    "@prisma/client": "^5.22.0",
    "@types/dedent": "^0.7.2",
    "@types/node": "^20.17.9",
    "changelogen": "^0.5.7",
    "eslint": "^9.16.0",
    "nuxt": "^3.14.1592",
    "prisma": "^5.22.0",
    "taze": "^0.18.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^1.6.0"
  },
  "eslintConfig": {
    "root": true,
    "ignorePatterns": [
      "**/dist/**",
      "**/.output/**",
      "**/.nuxt/**",
      "**/node_modules/**",
      "**/.history/**"
    ],
    "extends": [
      "@nuxt/eslint-config"
    ]
  }
}